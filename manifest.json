{
  "name": "Chrome Password Lock",
  "description": "Password-protect your entire Chrome Browser.",
  "version": "0.1.0",
  "manifest_version": 3,
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "host_permissions": ["<all_urls>"],
  "permissions": ["tabs", "activeTab"],
  "background": {
    "service_worker": "service_worker.js"
  }
}
